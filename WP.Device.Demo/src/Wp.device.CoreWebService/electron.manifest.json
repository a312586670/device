{
  "executable": "Wp.device.CoreWebService",
  "splashscreen": {
    "imageFile": ""
  },
  "devTools": false,
  "loadUrl": "http://localhost:8080/",
  "singleInstance": false,
  "build": {
    "appId": "com.Wp.device.CoreWebService.app",
    "productName": "Wp.device.CoreWebService",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}